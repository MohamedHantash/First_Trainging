@model List<ApplicationUser>
@{
    ViewData["Title"] = "Users Page";
}

<div class="text-center">
    <h1 class="display-4">Users Page</h1>
   
</div>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Age</td>
            <td>UserName</td>
            <td>Email</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>
    </thead>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Age</td>
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td><a asp-action="Edit" asp-controller="Home" asp-route-Id="@item.Id">Edit</a></td>
            <td><a asp-action="Delete" asp-controller="Home" asp-route-Id="@item.Id">Delete</a></td>
        </tr>
    }
</table>